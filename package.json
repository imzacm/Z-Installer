  "bugs": {
    "url": "https://github.com/imzacm/Z-Installer/issues"
  },
  "homepage": "https://github.com/imzacm/Z-Installer#readme",
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "daemonite-material": "^4.0.0-beta",
    "express": "^4.16.2",
    "jquery": "^3.2.1",
    "popper": "^1.0.0",
    "pug": "^2.0.0-rc.4",
    "tether": "^1.4.0",
    "snyk": "^1.42.7"
  },
  "snyk": true,
  "devDependencies": {
    "chai": "^4.1.2",
    "codacy-coverage": "^2.0.3",
    "electron": "^1.7.9",
    "electron-mocha": "^4.0.3",
